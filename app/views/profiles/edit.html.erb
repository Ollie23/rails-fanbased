<h1 class="text-center">Edit Profile</h1>
 <div class="container">
    <div class="row">
      <div class="col-xs-4 col-xs-offset-4">
        <%= simple_form_for @profile do |f| %>
            <%= f.input :username %>
            <%= f.input :first_name %>
            <%= f.input :last_name %>
            <%= f.input :age %>
            <%= f.submit %>
        <% end %>
    </div>
  </div>
</div>

