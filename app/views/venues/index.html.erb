

<div class="container">
  <div class="row">
  <h1>Venues</h1>

  <% @venues.each do |venue| %>
    <div class="col-xs-12 col-sm-6 col-md-3">
      <!-- div class="card" -->
      <div class = "text-center">
            <h3><%=venue.name%></h3>
            <%=venue.address%>
            <%= venue.description%>
            <%= venue.rating%>
            <%= venue.close%>
            <%= venue.screens%>
            <%= venue.internet%>
            <%= venue.food%>
            <%= venue.price_range%>

      </div>
    </div>
  <%end%>

  </div>
</div>
</div>

