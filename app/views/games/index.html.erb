<div class="container">
  <div class="row">
    <div class="col-xs-4 col-xs-offset-4">
      <ul>
      <h1>This week</h1>
        <%= will_paginate @games %>
           <% @games.each do |game| %>
              <p> <%= game.home_team.name +  " vs " + game.away_team.name %> </p>
              <p> <%=game.date_time%></p>
          <%end%>
        <%= will_paginate @games %>
      </ul>
    </div>
  </div>
</div>

