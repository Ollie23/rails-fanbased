
 <h1>This week</h1>
<div class="container">
  <div class="row">
    <div class="col-xs-4 col-xs-offset-4">
    <h1>This week</h1>
      <ul>
        <% @games.each do |game| %>
          <p> <%=game.date_time%></p>
          <p> <%= game.home_team.name +  " vs " + game.away_team.name %> </p>
          <hr>
        <%end%>
        <%= will_paginate @games %>
      </ul>
    </div>
  </div>
</div>


